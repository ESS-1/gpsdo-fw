                "CMAKE_BUILD_TYPE": "MinSizeRel",
                "EEPROM_AUTO_SAVE": "1"
            }
        },
        {
            "name": "MinSizeRel-NoAutoSave",
            "inherits": "default",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "MinSizeRel",
                "EEPROM_AUTO_SAVE": "0"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "Debug-AutoSave",
            "configurePreset": "Debug-AutoSave"
        },
        {
            "name": "Debug-NoAutoSave",
            "configurePreset": "Debug-NoAutoSave"
        },
        {
            "name": "RelWithDebInfo-AutoSave",
            "configurePreset": "RelWithDebInfo-AutoSave"
        },
        {
            "name": "RelWithDebInfo-NoAutoSave",
            "configurePreset": "RelWithDebInfo-NoAutoSave"
        },
        {
            "name": "Release-AutoSave",
            "configurePreset": "Release-AutoSave"
        },
        {
            "name": "Release-NoAutoSave",
            "configurePreset": "Release-NoAutoSave"
        },
        {
            "name": "MinSizeRel-AutoSave",
            "configurePreset": "MinSizeRel-AutoSave"
        },
        {
            "name": "MinSizeRel-NoAutoSave",
            "configurePreset": "MinSizeRel-NoAutoSave"
        }
    ]
}